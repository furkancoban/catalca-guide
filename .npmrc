# Optimize installation speed
prefer-offline=true
package-lock=true
save-exact=true
audit=false
fund=false

# Cache configuration
cache-min=3600
cache-max=86400

# Optimize network usage
network-timeout=300000
fetch-retries=5
fetch-retry-factor=2
fetch-retry-mintimeout=20000
fetch-retry-maxtimeout=120000 